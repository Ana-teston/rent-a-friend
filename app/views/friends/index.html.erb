<div class="container">
  <h1>All Friends</h1>

  <% @friends.each do |friend| %>
    <p>First name: <%= friend.first_name%></p>
    <p>Last name: <%= friend.last_name%></p>
    <p>Interests: <%= friend.interests %></p>
    <p>Start date: <%= friend.start_date %></p>
    <p>End date: <%= friend.end_date %></p>

  <%= link_to "Click Here", friend_path(friend.id) %>
  <% end %>
</div>
